= form_tag search_index_path, method: :get, class: 'navbar-form navbar-left' do
  = text_field_tag :query, @query, placeholder: 'Query', class: 'form-control'
  = select_tag :object, options_for_select(Search::OBJECTS, @object), class: 'form-control'
  = submit_tag 'Search ', class: 'btn btn-default'